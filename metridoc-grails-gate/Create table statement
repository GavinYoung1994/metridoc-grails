CREATE TABLE gate_door (door_id int, door_name VARCHAR(60), PRIMARY KEY(door_id));
CREATE TABLE gate_affiliation (affiliation_id int, affiliation_name VARCHAR(30), PRIMARY KEY(affiliation_id));
CREATE TABLE gate_center (center_id int, center_name VARCHAR(30), PRIMARY KEY(center_id));
CREATE TABLE gate_USC (USC_id int, USC_name VARCHAR(60), PRIMARY KEY(USC_id));
CREATE TABLE gate_department(department_id int, department_name VARCHAR(30), PRIMARY KEY(department_id));
CREATE TABLE gate_entry_record(
    entry_id int, 
    entry_datetime datetime,
    door int, 
    affiliation int, 
    center int, 
    USC int,
    department int,
    PRIMARY KEY (entry_id),
    FOREIGN KEY (door) REFERENCES gate_door(door_id),
    FOREIGN KEY (affiliation) REFERENCES gate_affiliation(affiliation_id),
    FOREIGN KEY (center) REFERENCES gate_center(center_id),
    FOREIGN KEY (USC) REFERENCES gate_USC(USC_id),
    FOREIGN KEY (department) REFERENCES gate_department(department_id)
);